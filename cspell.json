  "patterns": [
    {
      "name": "hex-colors",
      "pattern": "/#[0-9a-fA-F]{3,8}/g"
    },
    {
      "name": "urls",
      "pattern": "/https?:\\/\\/[^\\s]+/g"
    },
    {
      "name": "go-module-paths",
      "pattern": "/github\\.com\\/[\\w\\-\\.]+\\/[\\w\\-\\.]+/g"
    },
    {
      "name": "environment-variables",
      "pattern": "/\\$\\{[A-Z_]+\\}/g"
    },
    {
      "name": "docker-tags",
      "pattern": "/:[0-9]+\\.[0-9]+\\.[0-9]+(-[\\w\\-]+)?/g"
    }
  ],
